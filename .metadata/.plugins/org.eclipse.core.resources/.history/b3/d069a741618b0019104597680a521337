
public class Number2 {
	
	public int a;
	public int b;
	public int c;
	public int d;
	public Number2(int a, int b, int c, int d) {
		super();
		this.a = a;
		this.b = b;
		this.c = c;
		this.d = d;
	}
	public int getA() {
		return a;
	}
	public int getB() {
		return b;
	}
	public int getC() {
		return c;
	}
	public int getD() {
		return d;
	}
	@Override
	public String toString() {
		return "Number1 [a=" + a + ", b=" + b + ", c=" + c + ", d=" + d + "]";
	}
	
	
	
	
}


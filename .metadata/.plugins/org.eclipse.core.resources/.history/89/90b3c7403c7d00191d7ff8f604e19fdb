
public class WordsTrans {


	public static void main(String[]agrs) {
	
String s = "aaa BCde eKL MnoP";
String t = "";
for (int i = 0; i < s.length(); ++i) {
char ch = s.charAt(i);
if (!t.isEmpty()) {
t += " ";
}
int n = (int)ch - (int)'a' + 1;
t += String.valueOf(n);
}
System.out.println(t);
	
}
	

}

	
	
	// s 4 loverCase
	// s 5***
	

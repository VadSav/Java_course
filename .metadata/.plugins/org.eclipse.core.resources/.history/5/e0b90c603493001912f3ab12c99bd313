		Arrays.sort(args);
	
		 Comparator<Lines> comp = new ComparatorByLength();
	        Arrays.sort(word, comp.thenComparing(new ComparatorByLength()));
	        
	        for (int i = 0; i < word.length; i++) {
	            System.out.println(word[i]);
	        }
		
	        System.out.println("****************************************");
	}

